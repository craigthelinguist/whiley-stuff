
syntax "whiley" "\.whiley$"



# Functions, methods.
color cyan "\<(from|import|is|private|prot0ected|public)\>"
color brightcyan "\<(function|method|package|type)\>"
color brightCyan "(->)"

# Static checking.
color brightmagenta "\<(assert|assume|ensures|requires|where)\>"
color magenta "\<all|some\>"

# Control flow.
color green "\<(catch|else|if|switch|try|while)\>"
